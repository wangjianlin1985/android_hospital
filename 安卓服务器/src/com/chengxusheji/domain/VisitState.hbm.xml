<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="VisitState" table="VisitState">

          <!-- 主键状态id -->
          <id name="visitStateId" column="visitStateId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 出诊状态 -->
          <property
            name="visitStateName"
            column="visitStateName"
            type="string"
            length="20"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
